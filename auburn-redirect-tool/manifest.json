{
  "name": "ART",
  "version": "0.7",
  "description": "Auburn Redirect Tool allows simple URL replacement ciphers for domains frequently used by the CWS team.",
  "permissions": ["webRequest", "webRequestBlocking", "webNavigation",
                  "*://cws.auburn.edu/shared/*", "*://fp.auburn.edu/*", "*://fp/*", "*://cws/*", "*://oitapps/*", "*://dev/*", "*://cwsdev/*", "*://oitappsdev/*", "*://test/*", "*://cwstest/*", "*://oitappstest/*", "*://migr/*", "*://cwsmigr/*", "*://oitappsmigr/*", "*://auburn/*", "*://auaccess2.auburn.edu/*"],
  "background": {
    "scripts": ["art.js"]
  },
  "content_scripts": [
	{
		"js": ["auaccess.js"],
		"matches": ["*://auaccess2.auburn.edu/cp/home/displaylogin"]
	}
	],
  "page_action": {
    "default_icon": "icon19.png",
    "default_title": "ART"
  },
  "manifest_version": 2
}
